{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:report": "vitest run --reporter json --outputFile test-results.json"
  },
  "dependencies": {
    "jsdom": "^23.0.1",
    "mochawesome": "^7.1.3",
    "rete": "^2.0.2",
    "rete-area-plugin": "^2.0.1",
    "rete-auto-arrange-plugin": "^2.0.0",
    "rete-connection-plugin": "^2.0.0",
    "rete-context-menu-plugin": "^2.0.0",
    "rete-engine": "^2.0.0",
    "rete-render-utils": "^2.0.1",
    "rete-vue-plugin": "^2.0.4",
    "rete-vue-render-plugin": "^0.5.2",
    "three": "^0.157.0",
    "tone": "^14.7.77",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.17.17",
    "@types/three": "^0.156.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vitest": "^1.0.1",
    "vue-tsc": "^1.8.11"
  }
}